	month = oct,
	year = {2013},
	keywords = {biodiversity, Climate change, {DNA} barcoding, heathland, restoration ecology, surveillance monitoring, systematic conservation planning, targeted monitoring, tropical forest},
	pages = {1245--1257},
	file = {Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/PEF7RTAN/Ji et al. - 2013 - Reliable, verifiable and efficient monitoring of b.pdf:application/pdf;Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/FCGSCNHS/full.html:text/html}
