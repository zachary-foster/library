	author = {Stamatakis, A. and Ludwig, T. and Meier, H.},
	month = feb,
	year = {2005},
	pmid = {15608047},
	pages = {456--463},
	file = {Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/CAKSKKD3/Stamatakis et al. - 2005 - RAxML-III a fast program for maximum likelihood-b.pdf:application/pdf;Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/T76XKS7H/456.html:text/html}
}

@article{jia_comparisons_2006,
	title = {Comparisons of extraction and purification methods of soil microorganism {DNA} from rhizosphere soil},
	volume = {17},
	issn = {1007-662X, 1993-0607},
