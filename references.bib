	keywords = {Betulaceae, ectomycorrhizal fungal ({EMF}) community, Fagaceae, host range and specificity, internal transcribed spacer ({ITS}), mixed conifer–broadleaf forest, Pinaceae, terminal restriction fragment length polymorphism (T-{RFLP})},
	pages = {430--440},
	file = {Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/R7R8HN73/Ishida et al. - 2007 - Host effects on ectomycorrhizal fungal communities.pdf:application/pdf;Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/I8RQPMRE/abstract.html:text/html}
