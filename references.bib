	keywords = {evolution, internal transcribed spacers, {ITS}, Peronospora., Pythium, {rDNA}},
	pages = {17--32},
	file = {ScienceDirect Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/CGPN86V2/Cooke et al. - 2000 - A Molecular Phylogeny of Phytophthora and Related .pdf:application/pdf;ScienceDirect Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/JESH6SZC/S1087184500912027.html:text/html}
