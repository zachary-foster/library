	keywords = {cold, cold, light, light, photosynthesis, photosynthesis, redox sensing, redox sensing, temperature, temperature},
	pages = {224--230},
	file = {ScienceDirect Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/AMN6MJQR/Huner et al. - 1998 - Energy balance and acclimation to light and cold.pdf:application/pdf;ScienceDirect Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/899P5PZS/S1360138598012485.html:text/html}
}

