@article{liu_dna_2012,
	title = {{DNA} Barcode Goes Two-Dimensions: {DNA} {QR} Code Web Server},
	volume = {7},
	shorttitle = {{DNA} Barcode Goes Two-Dimensions},
	url = {http://dx.doi.org/10.1371/journal.pone.0035146},
	doi = {10.1371/journal.pone.0035146},
	abstract = {The {DNA} barcoding technology uses a standard region of {DNA} sequence for species identification and discovery. At present, “{DNA} barcode” actually refers to {DNA} sequences, which are not amenable to information storage, recognition, and retrieval. Our aim is to identify the best symbology that can represent {DNA} barcode sequences in practical applications. A comprehensive set of sequences for five {DNA} barcode markers {ITS}2, {rbcL}, {matK}, {psbA}-{trnH}, and {CO}1 was used as the test data. Fifty-three different types of one-dimensional and ten two-dimensional barcode symbologies were compared based on different criteria, such as coding capacity, compression efficiency, and error detection ability. The quick response ({QR}) code was found to have the largest coding capacity and relatively high compression ratio. To facilitate the further usage of {QR} code-based {DNA} barcodes, a web server was developed and is accessible at http://qrfordna.dnsalias.org. The web server allows users to retrieve the {QR} code for a species of interests, convert a {DNA} sequence to and from a {QR} code, and perform species identification based on local and global sequence similarities. In summary, the first comprehensive evaluation of various barcode symbologies has been carried out. The {QR} code has been found to be the most appropriate symbology for {DNA} barcode sequences. A web server has also been constructed to allow biologists to utilize {QR} codes in practical {DNA} barcoding applications.},
	number = {5},
	urldate = {2013-11-20},
	journal = {{PLoS} {ONE}},
	author = {Liu, Chang and Shi, Linchun and Xu, Xiaolan and Li, Huan and Xing, Hang and Liang, Dong and Jiang, Kun and Pang, Xiaohui and Song, Jingyuan and Chen, Shilin},
	month = may,
	year = {2012},
	pages = {e35146},
	file = {PLoS Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/3864KK3I/Liu et al. - 2012 - DNA Barcode Goes Two-Dimensions DNA QR Code Web S.pdf:application/pdf;PLoS Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/8IH6GNU2/infodoi10.1371journal.pone.html:text/html}
}

