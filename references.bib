	pages = {8053--8064},
	file = {PubMed Central Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/ICUTQD5W/Adams et al. - 2008 - Comparative Genome Sequence Analysis of Multidrug-.pdf:application/pdf}
