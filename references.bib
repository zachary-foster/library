	year = {2010},
	keywords = {454 massively parallel pyrosequencing, 454 massively parallel pyrosequencing, artifacts, artifacts, biodiversity, biodiversity, community structure, community structure, ectomycorrizal fungi, ectomycorrizal fungi, Korup National Park, Korup National Park, {PCR} bias, {PCR} bias},
