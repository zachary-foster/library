@article{ong_species_2013,
	title = {Species Identification and Profiling of Complex Microbial Communities Using Shotgun Illumina Sequencing of 16S {rRNA} Amplicon Sequences},
	volume = {8},
	issn = {1932-6203},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3620293/},
	doi = {10.1371/journal.pone.0060811},
	abstract = {The high throughput and cost-effectiveness afforded by short-read sequencing technologies, in principle, enable researchers to perform 16S {rRNA} profiling of complex microbial communities at unprecedented depth and resolution. Existing Illumina sequencing protocols are, however, limited by the fraction of the 16S {rRNA} gene that is interrogated and therefore limit the resolution and quality of the profiling. To address this, we present the design of a novel protocol for shotgun Illumina sequencing of the bacterial 16S {rRNA} gene, optimized to amplify more than 90\% of sequences in the Greengenes database and with the ability to distinguish nearly twice as many species-level {OTUs} compared to existing protocols. Using several in silico and experimental datasets, we demonstrate that despite the presence of multiple variable and conserved regions, the resulting shotgun sequences can be used to accurately quantify the constituents of complex microbial communities. The reconstruction of a significant fraction of the 16S {rRNA} gene also enabled high precision ({\textgreater}90\%) in species-level identification thereby opening up potential application of this approach for clinical microbial characterization.},
	number = {4},
	urldate = {2014-02-10},
	journal = {{PLoS} {ONE}},
	author = {Ong, Swee Hoe and Kukkillaya, Vinutha Uppoor and Wilm, Andreas and Lay, Christophe and Ho, Eliza Xin Pei and Low, Louie and Hibberd, Martin Lloyd and Nagarajan, Niranjan},
	month = apr,
	year = {2013},
	pmid = {23579286},
	pmcid = {PMC3620293},
	file = {PubMed Central Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/HH6MT6JI/Ong et al. - 2013 - Species Identification and Profiling of Complex Mi.pdf:application/pdf}
}

@article{wilson_temperature_2001,
	title = {Temperature induction of viruses in symbiotic dinoflagellates},
	volume = {25},
	url = {http://www.int-res.com/abstracts/ame/v25/n1/p99-102/},
	doi = {10.3354/ame025099},
	abstract = {{ABSTRACT}: Bleaching manifests itself as a loss of symbiotic dinoflagellates (zooxanthellae) and/or chlorophyll from a variety of symbiotic hosts, including corals and sea anemones. Bleaching is known to result from a range of environmental stresses, the most significant of which is elevated temperature; how these stresses elicit a bleaching response is currently the focus of intense research. One consequence of environmental stress that has yet to be considered is viral attack. Here, we have isolated a transferable infectious agent believed to be a virus, from zooxanthellae of the temperate sea anemone Anemonia viridis. The infectious agent is induced by elevated temperature. Once induced, the filterable agent can be further propagated without heat induction, thus fulfilling Koch¹s postulates. We propose that zooxanthellae harbor a latent viral infection that is induced by exposure to elevated temperatures. If such a mechanism also operates in the zooxanthellae harbored by reef corals, and these viruses kill the symbionts, then this could contribute to temperature-induced bleaching.},
	number = {1},
	urldate = {2012-11-20},
	journal = {Aquatic Microbial Ecology},
	author = {Wilson, William H. and Francis, Isobel and Ryan, Keith and Davy, Simon K.},
	month = aug,
	year = {2001},
	pages = {99--102},
	file = {Aquatic Microbial Ecology Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/QBA9IBVI/Wilson et al. - 2001 - Temperature induction of viruses in symbiotic dino.pdf:application/pdf;Aquatic Microbial Ecology Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/DUTRT2CP/p99-102.html:text/html}
}

