	year = {1997},
	keywords = {{DNA} extraction, Fragaria, Plant Physiology, Plant Sciences, {RAPD}, strawberry},
	pages = {8--15},
	file = {Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/HRIKAWNE/Porebski et al. - 1997 - Modification of a CTAB DNA extraction protocol for.pdf:application/pdf;Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/I6IE85AX/BF02772108.html:text/html}
