@article{brown_isotopically-depleted_2010,
	title = {Isotopically-depleted late Pleistocene groundwater in Columbia River Basalt aquifers: Evidence for recharge of glacial Lake Missoula floodwaters?},
	volume = {37},
	copyright = {Copyright 2010 by the American Geophysical Union.},
	issn = {1944-8007},
	shorttitle = {Isotopically-depleted late Pleistocene groundwater in Columbia River Basalt aquifers},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2010GL044992/abstract},
	doi = {10.1029/2010GL044992},
	abstract = {Late Pleistocene outburst flooding of ice-dammed glacial Lake Missoula, and possible discharge from the Cordilleran Ice Sheet ({CIS}), catastrophically altered the northwestern United States landscape, yet little is known about potential infiltration of flood waters into the subsurface. This study provides compelling evidence for the presence of late Pleistocene {CIS}-related recharge waters in the Columbia River Basalt Aquifers ({CRBAs}) in central Washington. {CRBA} groundwaters with corrected 14C ages from 15.7 and 33.3 k yrs {BP} (during periods of flood events) have anomalously low δ18O values (−18.9 to −17.6‰), compared to late Pleistocene soil waters (−16.1 to −13.4‰) and modern precipitation in the region (average −15.9‰), consistent with {CIS}-related meltwater recharge. These results have implications for our understanding of megaflood phenomena on earth and Mars.},
	language = {en},
	number = {21},
	urldate = {2013-07-22},
	journal = {Geophysical Research Letters},
	author = {Brown, Kyle B. and McIntosh, Jennifer C. and Baker, Victor R. and Gosch, Damian},
	year = {2010},
	keywords = {groundwater, groundwater, hydrology, hydrology, Missoula floods, Missoula floods, oxygen isotopes, oxygen isotopes, Pleistocene, Pleistocene, recharge, recharge},
	pages = {n/a--n/a},
	file = {Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/VEPG6N5T/Brown et al. - 2010 - Isotopically-depleted late Pleistocene groundwater.pdf:application/pdf;Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/2REVCSKW/full.html:text/html}
}

@article{giessler_capturing_2013,
	title = {Capturing the population structure of microparasites: using {ITS}-sequence data and a pooled {DNA} approach},
	volume = {13},
	copyright = {© 2013 John Wiley \& Sons Ltd},
	issn = {1755-0998},
	shorttitle = {Capturing the population structure of microparasites},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/1755-0998.12144/abstract},
	doi = {10.1111/1755-0998.12144},
	abstract = {The internal transcribed spacer ({ITS}) region of nuclear ribosomal {DNA} is a common marker not only for the molecular identification of different taxa and strains, but also for the analysis of population structure of wild microparasite communities. Importantly, the multicopy nature of this region allows the amplification of low-quantity samples of the target {DNA}, a common problem in studies on unicellular, unculturable microparasites. We analysed {ITS} sequences from the protozoan parasite Caullerya mesnili (class Ichthyosporea) infecting waterflea (Daphnia) hosts, across several host population samples. We showed that analysing representative {ITS}-types [as identified by statistical parsimony network analysis ({SPN})] is a suitable method to address relevant polymorphism. The spatial patterns were consistent regardless of whether parasite {DNA} was extracted from individual hosts or pooled host samples. Remarkably, the efficiency in detecting different sequence types was even higher after sample pooling. As shown by simulations, an easily manageable number of sequences from pooled {DNA} samples are sufficient to resolve the spatial population structure in this system. In summary, the {ITS} region analysed from pooled {DNA} samples can provide valuable insights into the spatial and temporal dynamics of microparasites. Moreover, the application of {SPN} analysis is a good alternative to the well-established neighbour-joining method ({NJ}) for the identification of representative {ITS}-types. {SPN} can even outperform {NJ} by joining most of the singleton sequences to representative sequence clusters.},
	language = {en},
	number = {5},
	urldate = {2013-11-18},
	journal = {Molecular Ecology Resources},
	author = {Giessler, Sabine and Wolinska, Justyna},
	year = {2013},
	keywords = {Caullerya mesnili, Caullerya mesnili, {DNA} pooling, {DNA} pooling, Ichthyosporea, Ichthyosporea, intragenomic variation, intragenomic variation, {ITS} region, {ITS} region, population structure, population structure, statistical parsimony network analysis, statistical parsimony network analysis},
	pages = {918--928},
	file = {Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/9BU2GMBI/Giessler and Wolinska - 2013 - Capturing the population structure of microparasit.pdf:application/pdf;Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/DH9UEHJ5/abstract.html:text/html}
}

@article{carini_discovery_2014,
	title = {Discovery of a {SAR}11 growth requirement for thiamin’s pyrimidine precursor and its distribution in the Sargasso Sea},
	copyright = {© 2014 Nature Publishing Group},
	issn = {1751-7362},
	url = {http://www.nature.com/ismej/journal/vaop/ncurrent/full/ismej201461a.html},
	doi = {10.1038/ismej.2014.61},
	abstract = {Vitamin traffic, the production of organic growth factors by some microbial community members and their use by other taxa, is being scrutinized as a potential explanation for the variation and highly connected behavior observed in ocean plankton by community network analysis. Thiamin (vitamin B1), a cofactor in many essential biochemical reactions that modify carbon–carbon bonds of organic compounds, is distributed in complex patterns at subpicomolar concentrations in the marine surface layer (0–300 m). Sequenced genomes from organisms belonging to the abundant and ubiquitous {SAR}11 clade of marine chemoheterotrophic bacteria contain genes coding for a complete thiamin biosynthetic pathway, except for {thiC}, encoding the 4-amino-5-hydroxymethyl-2-methylpyrimidine ({HMP}) synthase, which is required for de novo synthesis of thiamin’s pyrimidine moiety. Here we demonstrate that the {SAR}11 isolate ‘Candidatus Pelagibacter ubique’, strain {HTCC}1062, is auxotrophic for the thiamin precursor {HMP}, and cannot use exogenous thiamin for growth. In culture, strain {HTCC}1062 required 0.7 zeptomoles per cell (ca. 400 {HMP} molecules per cell). Measurements of dissolved {HMP} in the Sargasso Sea surface layer showed that {HMP} ranged from undetectable (detection limit: 2.4 {pM}) to 35.7 {pM}, with maximum concentrations coincident with the deep chlorophyll maximum. In culture, some marine cyanobacteria, microalgae and bacteria exuded {HMP}, and in the Western Sargasso Sea, {HMP} profiles changed between the morning and evening, suggesting a dynamic biological flux from producers to consumers.},
	language = {en},
	urldate = {2014-05-16},
	journal = {The {ISME} Journal},
	author = {Carini, Paul and Campbell, Emily O. and Morré, Jeff and Sañudo-Wilhelmy, Sergio A. and Cameron Thrash, J. and Bennett, Samuel E. and Temperton, Ben and Begley, Tadhg and Giovannoni, Stephen J.},
	month = apr,
	year = {2014},
	keywords = {auxotrophy, B1, micronutrient, phytoplankton, thiamine, vitamins},
	file = {Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/ZWF5AR7T/Carini et al. - 2014 - Discovery of a SAR11 growth requirement for thiami.pdf:application/pdf;Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/X6FTV3HV/ismej201461a.html:text/html}
}

