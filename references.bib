@article{jargeat_phylogenetic_2010,
	title = {Phylogenetic species delimitation in ectomycorrhizal fungi and implications for barcoding: the case of the Tricholoma scalpturatum complex (Basidiomycota)},
	volume = {19},
	copyright = {© 2010 Blackwell Publishing Ltd},
	issn = {1365-294X},
	shorttitle = {Phylogenetic species delimitation in ectomycorrhizal fungi and implications for barcoding},
	url = {http://onlinelibrary.wiley.com.ezproxy.proxy.library.oregonstate.edu/doi/10.1111/j.1365-294X.2010.04863.x/abstract},
	doi = {10.1111/j.1365-294X.2010.04863.x},
	abstract = {Population studies have revealed that the fungal ectomycorrhizal morphospecies Tricholoma scalpturatum consists of at least two genetically distinct groups that occur sympatrically in several geographical areas. This discovery prompted us to examine species boundaries and relationships between members formerly assigned to T. scalpturatum and allied taxa using phylogenetic analyses. Sequence data were obtained from three nuclear {DNA} regions [internal transcribed spacer ({ITS}), gpd and tef], from 101 carpophores collected over a large geographical range in Western Europe, and some reference sequences from public databases. The {ITS} was also tested for its applicability as {DNA} barcode for species delimitation. Four highly supported phylogenetic clades were detected. The two previously detected genetic groups of T. scalpturatum were assigned to the phylospecies Tricholoma argyraceum and T. scalpturatum. The two remaining clades were referred to as Tricholoma cingulatum and Tricholoma inocybeoides. Unexpectedly, T. cingulatum showed an accelerated rate of evolution that we attributed to narrow host specialization. This study also reveals recombinant {ITS} sequences in T. inocybeoides, suggesting a hybrid origin. The {ITS} was a useful tool for the determination of species boundaries: the mean value of intraspecific genetic distances in the entire {ITS} region (including 5.8S {rDNA}) was {\textless}0.2\%, whereas interspecific divergence estimates ranged from 1.78\% to 4.22\%. Apart from giving insights into the evolution of the T. scalpturatum complex, this study contributes to the establishment of a library of taxonomically verified voucher specimens, an a posteriori correlation between phenotype and genotype, and {DNA} barcoding of ectomycorrhizal fungi.},
	language = {en},
	number = {23},
	urldate = {2014-03-20},
	journal = {Molecular Ecology},
	author = {Jargeat, Patricia and Martos, Florent and Carriconde, Fabian and Gryta, Hervé and Moreau, Pierre-Arthur and Gardes, Monique},
	month = dec,
	year = {2010},
	keywords = {{DNA} barcode, ectomycorrhizal fungi, phylogenetic species recognition, recombinant {ITS}, Tricholoma scalpturatum},
	pages = {5216--5230},
	file = {Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/58288AU7/Jargeat et al. - 2010 - Phylogenetic species delimitation in ectomycorrhiz.pdf:application/pdf;Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/KEW9FRFN/abstract.html:text/html}
}

@article{manter_use_2007,
	title = {Use of the {ITS} primers, {ITS}1F and {ITS}4, to characterize fungal abundance and diversity in mixed-template samples by {qPCR} and length heterogeneity analysis},
	volume = {71},
	issn = {0167-7012},
	url = {http://www.sciencedirect.com/science/article/pii/S0167701207002424},
	doi = {10.1016/j.mimet.2007.06.016},
	abstract = {Molecular-based approaches to assess microbial biomass and diversity from soil and other ecosystems are rapidly becoming the standard methodology for analysis. While these techniques are advantageous, because they do not rely on the need to culture organisms, each technique may have its own biases and/or limitations when used to assess fungal diversity from mixed-template samples. In this study, we analyzed {PCR} specificity and efficiency of the {ITS} primers ({ITS}1F and {ITS}4) in a series of single- and mixed-template samples using a combined quantitative {PCR}–length heterogeneity analysis ({LH}-{qPCR}) approach. As expected, these primers successfully amplified all higher fungal species tested (10 ascomycetes, 6 basidiomycetes, and 4 zygomycetes) and no members of the oomycetes. Based on our results, and a search of the {GenBank} database, amplicons of the {ITS}1F and {ITS}4 primer set exhibit considerable variability (420 to 825 bp), but due to similarities in amplicon sizes of some fungal species, actual species diversity in environmental samples may be underestimated approximately two-fold. The addition of an initial {qPCR} step allowed for the accurate quantitation of total fungal {DNA} in mixed-template samples over five orders of magnitude (10− 1 to 103 pg μl− 1). {PCR} biases between individuals in mixed-templates rendered it impossible to determine the absolute quantity of any individual within a population from its individual peak height. However, relative changes in individuals within a mixed-template sample could be determined due to a constant proportionality between peak heights and starting template concentration. Variability associated with the individual steps of the {LH}-{qPCR} analysis was also determined from environmental samples.},
	number = {1},
	urldate = {2013-07-18},
	journal = {Journal of Microbiological Methods},
	author = {Manter, Daniel K. and Vivanco, Jorge M.},
	month = oct,
	year = {2007},
	keywords = {Diversity, Diversity, {DNA}, {DNA}, fungi, fungi, Molecular ecology, Molecular ecology, Soil, Soil},
	pages = {7--14},
	file = {ScienceDirect Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/BAZ4E5GZ/Manter and Vivanco - 2007 - Use of the ITS primers, ITS1F and ITS4, to charact.pdf:application/pdf;ScienceDirect Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/BEJ58H3U/S0167701207002424.html:text/html}
}

