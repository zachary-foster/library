	file = {Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/UB876PME/Avis et al. - 2010 - Pooled samples bias fungal community descriptions.pdf:application/pdf;Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/X6R2KMKZ/abstract.html:text/html}
}

