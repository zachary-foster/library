	author = {Manter, Daniel K. and Delgado, Jorge A. and Holm, David G. and Stong, Rachel A.},
	month = jul,
	year = {2010},
	keywords = {ecology, ecology, Geoecology/Natural Processes, Geoecology/Natural Processes, Microbial Ecology, Microbial Ecology, Microbiology, Microbiology, Nature Conservation, Nature Conservation},
	pages = {157--166},
	file = {Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/PUQBG7HC/Manter et al. - 2010 - Pyrosequencing Reveals a Highly Diverse and Cultiv.pdf:application/pdf;Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/CTZ7KEH7/10.html:text/html}
}

