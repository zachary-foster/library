@article{wang_environmental_2013,
	title = {Environmental bio-monitoring with high-throughput sequencing},
	volume = {14},
	issn = {1467-5463, 1477-4054},
	url = {http://bib.oxfordjournals.org/content/14/5/575},
	doi = {10.1093/bib/bbt032},
	abstract = {There is much interest in using high-throughput {DNA} sequencing methodology to monitor microorganisms, complex plant and animal communities. However, there are experimental and analytical issues to consider before applying a sequencing technology, which was originally developed for genome projects, to ecological projects. Many of these issues have been highlighted by recent microbial studies. Understanding how high-throughput sequencing is best implemented is important for the interpretation of recent results and the success of future applications. Addressing complex biological questions with metagenomics requires the interaction of researchers who bring different skill sets to problem solving. Educators can help by nurturing a collaborative interdisciplinary approach to genome science, which is essential for effective problem solving. Educators are in a position to help students, teachers, the public and policy makers interpret the new knowledge that metagenomics brings. To do this, they need to understand, not only the excitement of the science but also the pitfalls and shortcomings of methodology and research designs. We review these issues and some of the research directions that are helping to move the field forward.},
	language = {en},
	number = {5},
	urldate = {2014-04-16},
	journal = {Briefings in Bioinformatics},
	author = {Wang, Jing and McLenachan, Patricia A. and Biggs, Patrick J. and Winder, Linton H. and Schoenfeld, Barbara I. K. and Narayan, Vinay V. and Phiri, Bernard J. and Lockhart, Peter J.},
	month = sep,
	year = {2013},
	pmid = {23677899},
	keywords = {environmental bio-monitoring, high-throughput sequencing, Metagenomics},
	pages = {575--588},
	file = {Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/F5VXBS36/575.html:text/html}
}

