	pmid = {2413450},
	pages = {6955--6959},
	file = {Full Text PDF:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/WTNF6845/Lane et al. - 1985 - Rapid determination of 16S ribosomal RNA sequences.pdf:application/pdf;Snapshot:/home/local/USDA-ARS/fosterz/.zotero/zotero/0ilnjngu.default/zotero/storage/HMU2PQAZ/6955.html:text/html}
